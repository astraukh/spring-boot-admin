<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

    <!-- Suppress for test classes and test configs -->
    <suppress checks="MethodName" files=".*Test.java"/>
    <suppress checks="ClassFanOutComplexity" files=".*Test.java"/>
    <suppress checks="FileLength" files=".*Test.java"/>
    <suppress checks="MethodLength" files=".*Test.java"/>
    <suppress checks="ClassFanOutComplexity" files=".*TestConfig.java"/>
    <suppress checks="ClassDataAbstractionCoupling" files=".*TestConfig.java"/>

</suppressions>
